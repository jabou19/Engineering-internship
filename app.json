{
  "expo": {
    "name": "retireditems",
    "slug": "retireditems",
    "version": "1.0.0",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "infoPlist": {
        "NSLocationAlwaysAndWhenInUseUsageDescription": "REASON_FOR_REQUEST",
        "UIBackgroundModes": [
          "location",
          "fetch"
        ]
      }
    },
    "android": {
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "ACCESS_BACKGROUND_LOCATION"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "REPLACE_FOR_API_KEY"
        }
      }
    },
    "sdkVersion": "49.0.0",
    "entryPoint": ".vscode\\exponentIndex.js",
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Uppdrop to access your camera."
        }
      ]
    ]
  },
  "name": "main",
  "displayName": "retireditems"
}